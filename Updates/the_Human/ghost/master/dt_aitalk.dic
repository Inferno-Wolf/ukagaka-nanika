//-OnMinuteChange

//Keeps track of if an hour has passed. Just ignore this.
HourCheck
{
	if passmin >= 59
	{
		passmin = 0
		passhour = passhour + 1
	}
}

//This is what will trigger time-based dialogue for your Ghost.
OnMinuteChange
{
	HourCheck
	--
 	if minute == 0
	{
		if hourchecks == 1 //on
		{
			if hour == 12 //noon time
			{
				"\0*It's noon.\w8\e"
				
				"\0*It's 12'o clock,\w4 noon.\w8\e"
				
				"\0*It's noon'o clock.\w8\e"
			}
			elseif hour == 0 //midnight time
			{
				"\0*It's midnight.\w8\e"
				
				"\0*It's 12'o clock,\w4 midnight.\w8\e"
				
				"\0*It's midnight'o clock.\w8\e"
			}
			else
			{
				"\0*It's %(hour12)'o clock.\e"
			}
		}
		else //0; off
		{
			""
		}
	}
	elseif mikireflag == 1
	{
		MikireTalk
	}
}

//--OnSecondChange
OnSecondChange
{
	if passsec == 59
 	{
	 	passsec = 0
 		passmin = passmin + 1
	}
 	else
 	{
	 	passsec = passsec + 1
	}
	//Dialogue for putting them off the screen.
	if mikireflag == 0 && reference1 == 1
	{
		"\0\s[3]*You've moved Frisk offscreen.\w8\e"
		
		MikireTalk
	}
	//Dialogue for coming back on the screen
	if mikireflag == 1 && reference1 == 0
	{
		EndMikireTalk
	}
	
	mikireflag = reference1 //reference1 is if the ghost is offscreen or not
 	
	//This dialogue is for when the two characters are overlapping each other.
	if reference2 == 1
	{
 		"\0\s[1]*Frisk and Flowey are somehow overlapping.\w8\1\s[19]*Your resolution is %screenwidthx%screenheight,\w4 %(username). \4Shouldn't we have enough space?\w8\e"
	}
}
	// %screenwidthx%screenheight
	// \4 -> moves away; \5 -> moves towards (if they can; if too close, also moves away)

//---- MikireTalk ---------------------------------------------------------------------
//Note the hotkey combo Ctrl-Shift-F12. It brings the main character back onscreen.
MikireTalk
{
	"\0\s[3]*Frisk is offscreen,\w4 %(username).\w8\e"
	
	"\0\s[31]*Frisk is offscreen,\w4 %(username).\w8\e"
}

//----EndMikireTalk--------------------------------------------------------------------
EndMikireTalk
{
	"\0\s[6]*Frisk has returned from the void.\w8\e"
	
	"\0\s[7]*Frisk has been brought back into view.\w8\e"
}


