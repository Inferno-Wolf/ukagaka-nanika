//**** OnSurfaceRestore *****************************************************************
OnSurfaceRestore
{
 	if reference0 == 9
 	{
		"\0\s[9]\e"
 	}
	elseif reference1 == 15
 	{
		"\1\s[10]\0\s[7]\e"
 	}
 	elseif reference1 == 11 || reference1 == 12 || reference1 == 17 || reference1 == 18 || reference1 == 19
 	{
		"\1\s[19]\w8\s[-1]\0\s[0]\e"
 	}
 	else
 	{
		"\0\s[0]\1\s[10]\e"
	}
}


//--OnKeyPress
OnKeyPress
{
	if reference0 == "t"
	{OnAiTalk}
	elseif reference0 == "r"
	{OnLastTalk}
	elseif reference0 == "f1"
	{"\![open,readme]"}
}

